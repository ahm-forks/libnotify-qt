set(@PACKAGE_NAME@_VERSION @PROJECT_VERSION@)

@PACKAGE_INIT@

find_package(Qt@SELECTED@ COMPONENTS Core DBus Gui REQUIRED)

set_and_check(@PACKAGE_NAME@_INCLUDE_DIRS "@PACKAGE_CMAKE_INSTALL_INCLUDEDIR@/@PACKAGE_NAME@")

include("${CMAKE_CURRENT_LIST_DIR}/@PACKAGE_NAME@Targets.cmake")

list(APPEND @PACKAGE_NAME@_INCLUDE_DIRS
	${Qt@SELECTED@Core_INCLUDE_DIRS}
	${Qt@SELECTED@DBus_INCLUDE_DIRS}
	${Qt@SELECTED@Gui_INCLUDE_DIRS}
)

check_required_components(@PACKAGE_NAME@)
